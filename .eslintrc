{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"no-unused-expressions": "warn",
		"no-unused-labels": "warn",
		"no-unused-vars": "warn",
		"no-undef": "error",
		"spaced-comment": "warn",
		"prefer-destructuring": "warn",
		"no-lone-blocks": "warn",
		"no-self-compare": "warn",
		"no-mixed-spaces-and-tabs": "off"
	}
}